@model Fitness.Web.ViewModels.Diet.AllViewModel

@{
    this.ViewData["Title"] = "All";
}

<h1>@this.ViewData["Title"]</h1>

<h2>eheel</h2>

<div class="row">
    <div class="media col-md-4">
        <ul>
            @foreach (var diet in Model.Diets)
            {
                <li>
                    <p>@diet.Name</p>
                    <p>@diet.Duration</p>
                    <p>@diet.Description</p>
                </li>
            }
        </ul>
    </div>
</div>